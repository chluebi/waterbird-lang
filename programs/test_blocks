
main() {
    l = [0, 0, 0, 0, 0];

    while (l[-1] == 0) {
        i = 0;
        l[
            #{
                while (l[i] != 0) {
                    i++;
                }
                i
            }
        ] = #{
            if (i < len(l)-1) {
                i++;
            }
            while (l[i] != 0) {
                i--;
            }
            i
        };
    }
    

    l[-1]
}
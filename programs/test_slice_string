main() {
    assert("123" == "123");
    assert("123"[:] == "123");
    assert("123"[::] == "123");
    assert("123"[0:] == "123");
    assert("123"[:len("123"):] == "123");
    assert("123"[0::1] == "123");
    assert("123"[:len("123"):1] == "123");

    assert("123"[::-1] == "321");
    assert("123"[len("123")::-1] == "321");
    assert("123"[:0:-1] == "321");
    assert("123"[len("123")::-1] == "321");

    assert("123"[:1:] == "1");
    assert("123"[0:1:] == "1");
    assert("123"[1:2:] == "2");
    assert("123"[2:3:] == "3");

    assert("123"[1::-1] == "1");
    assert("123"[1:0:-1] == "1");
    assert("123"[2:1:-1] == "2");
    assert("123"[3:2:-1] == "3");

    assert("123"[:2:] == "12");
    assert("123"[0:2:] == "12");
    assert("123"[1:3:] == "23");

    assert("123"[2::-1] == "21");
    assert("123"[2:0:-1] == "21");
    assert("123"[3:1:-1] == "32");

    assert("123"[:3:] == "123");
    assert("123"[0:3:] == "123");

    assert("123"[3::-1] == "321");
    assert("123"[3:0:-1] == "321");

    assert("12345"[::2] == "135");
    assert("12345"[::3] == "14");
    assert("12345"[::4] == "15");

    assert("12345"[::-2] == "531");
    assert("12345"[::-3] == "41");
    assert("12345"[::-4] == "51");

    assert("12345"[1::2] == "24");
    assert("12345"[1::3] == "25");
    assert("12345"[1::4] == "2");

    assert("12345"[:1:-2] == "42");
    assert("12345"[:1:-3] == "52");
    assert("12345"[:1:-4] == "2");

    assert("12345"[1:4:2] == "24");
    assert("12345"[1:4:3] == "2");
    assert("12345"[1:4:4] == "2");

    assert("12345"[4:1:-2] == "42");
    assert("12345"[4:1:-3] == "2");
    assert("12345"[4:1:-4] == "2");

    assert("12345"[1:3:2] == "2");
}
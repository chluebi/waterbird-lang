get_first_int(l) {
    for c in l {
        if c in "1234567890" {
            return int(c);
        }
    }
}

rev(l) {
    new_l = l.clone();
    ret_l = [];
    while (len(new_l) > 0) {
        ret_l.push(new_l.pop());
    }
    ret_l
}

main() {
    inp = read("test_data/aoc_2023_1");
    s = 0;
    for line in split(inp, "\n") {
        s += int(str(get_first_int(line)) + str(get_first_int(rev(list(line)))));
    }
    s
}
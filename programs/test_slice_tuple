main() {
    assert((1, 2, 3) == (1, 2, 3));
    assert((1, 2, 3)[:] == (1, 2, 3));
    assert((1, 2, 3)[::] == (1, 2, 3));
    assert((1, 2, 3)[0:] == (1, 2, 3));
    assert((1, 2, 3)[:len((1, 2, 3)):] == (1, 2, 3));
    assert((1, 2, 3)[0::1] == (1, 2, 3));
    assert((1, 2, 3)[:len((1, 2, 3)):1] == (1, 2, 3));

    assert((1, 2, 3)[::-1] == (3, 2, 1));
    assert((1, 2, 3)[len((1, 2, 3))::-1] == (3, 2, 1));
    assert((1, 2, 3)[:0:-1] == (3, 2, 1));
    assert((1, 2, 3)[len((1, 2, 3))::-1] == (3, 2, 1));

    assert((1, 2, 3)[:1:] == (1,));
    assert((1, 2, 3)[0:1:] == (1,));
    assert((1, 2, 3)[1:2:] == (2,));
    assert((1, 2, 3)[2:3:] == (3,));

    assert((1, 2, 3)[1::-1] == (1,));
    assert((1, 2, 3)[1:0:-1] == (1,));
    assert((1, 2, 3)[2:1:-1] == (2,));
    assert((1, 2, 3)[3:2:-1] == (3,));

    assert((1, 2, 3)[:2:] == (1, 2));
    assert((1, 2, 3)[0:2:] == (1, 2));
    assert((1, 2, 3)[1:3:] == (2, 3));

    assert((1, 2, 3)[2::-1] == (2, 1));
    assert((1, 2, 3)[2:0:-1] == (2, 1));
    assert((1, 2, 3)[3:1:-1] == (3, 2));

    assert((1, 2, 3)[:3:] == (1, 2, 3));
    assert((1, 2, 3)[0:3:] == (1, 2, 3));

    assert((1, 2, 3)[3::-1] == (3, 2, 1));
    assert((1, 2, 3)[3:0:-1] == (3, 2, 1));

    assert((1, 2, 3, 4, 5)[::2] == (1, 3, 5));
    assert((1, 2, 3, 4, 5)[::3] == (1, 4));
    assert((1, 2, 3, 4, 5)[::4] == (1, 5));

    assert((1, 2, 3, 4, 5)[::-2] == (5, 3, 1));
    assert((1, 2, 3, 4, 5)[::-3] == (4, 1));
    assert((1, 2, 3, 4, 5)[::-4] == (5, 1));

    assert((1, 2, 3, 4, 5)[1::2] == (2, 4));
    assert((1, 2, 3, 4, 5)[1::3] == (2, 5));
    assert((1, 2, 3, 4, 5)[1::4] == (2,));

    assert((1, 2, 3, 4, 5)[:1:-2] == (4, 2));
    assert((1, 2, 3, 4, 5)[:1:-3] == (5, 2));
    assert((1, 2, 3, 4, 5)[:1:-4] == (2,));

    assert((1, 2, 3, 4, 5)[1:4:2] == (2, 4));
    assert((1, 2, 3, 4, 5)[1:4:3] == (2,));
    assert((1, 2, 3, 4, 5)[1:4:4] == (2,));

    assert((1, 2, 3, 4, 5)[4:1:-2] == (4, 2));
    assert((1, 2, 3, 4, 5)[4:1:-3] == (2,));
    assert((1, 2, 3, 4, 5)[4:1:-4] == (2,));

    assert((1, 2, 3, 4, 5)[1:3:2] == (2,));
}